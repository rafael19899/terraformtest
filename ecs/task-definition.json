{
  "family": "my-microservice",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "my-microservice",
      "image": "my-microservice:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}

